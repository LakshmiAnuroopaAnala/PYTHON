l=input().split()  #Enter alternative binary array as no 1's are consecutive but 0's may consecutive
n=int(input())     #enter number of flower pots we want to place
for i in range(0,len(l)-2):
    if l[i]=='0' and l[i+1]=='0' and l[i+2]=='0':
        n-=1
if n==0:
    print("s")
else:
    print("n")
