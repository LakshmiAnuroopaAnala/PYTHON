def Swasthik(n):
    for i in range(0,n):
        for j in range(0,n):
            if (i<n//2 and j==0) or (i==n//2) or(i>n//2 and j==n-1) or (j==n//2) or (i==0 and j>n//2) or (i==n-1 and j<n//2) :
                print("*",end="")
            else:
                print(" ",end="")
        print()
n=int(input())
Swasthik(n)
