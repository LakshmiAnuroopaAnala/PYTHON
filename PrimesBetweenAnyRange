import math
def prime(n):
    sq=int(math.sqrt(i))
    for j in range(2,sq+1):
        if n%j == 0:
           break
    else:
        print(i)
n1=int(input("Enter Lower Range - "))
n2=int(input("Enter Upper Range - "))
if n1<n2:
    for i in range(n1,n2+1):
        prime(i)
else:
    for i in range(n1,n2-1,-1):
        prime(i)
